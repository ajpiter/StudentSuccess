CREATE TABLE UF_R3_ANALYSIS_MAXBEG2007 AS(
select *
from UFDL_PWCBA_DATA.uf_r3_analysis_date2007v2 A
WHERE A.TERM_BEG_DT_SID = (SELECT MAX(B.TERM_BEG_DT_SID)
FROM UFDL_PWCBA_DATA.uf_r3_analysis_date2007v2 B
WHERE A.PERSON_SID = B.PERSON_SID
)
)

create table UF_R3_ANALYSIS_MAXBEGSU2007 AS(
select *
from UFDL_PWCBA_DATA.uf_r3_analysis_maxbeg2007 A
WHERE A.STUDENT_SUCCESSNO = (SELECT MAX(B.STUDENT_SUCCESSNO)
FROM UFDL_PWCBA_DATA.uf_r3_analysis_maxbeg2007 B
WHERE A.PERSON_SID = B.PERSON_SID
)
)
