create table UF_R3_ANALYSIS_LASTMAJOR0714BE AS(
SELECT DISTINCT
A.TERM_SID,
A.AGE_YEARS,
A.AGE_MONTHS,
A.AGE_DAYS,
A.TERM_BEG_DT_SID,
B.TERM_END_DT_SID,
A.JUNIOR_SENIOR_FLAGNO,
A.TOT_TRNSFR,
A.TOT_TEST_CREDIT,
A.UNT_TAKEN_PRGRSS,
A.CUM_GPA,
A.ENRL_CNT,
A.MAJORCOUNT,
A.UF_CLASSNO,
A.RESIDENCYNO,
A.TOT_TAKEN_GPA,
A.ACAD_PROG_PRIMARYNO,
A.UF_CLASS_EOTNO,
A.UNT_TAKEN_GPA,
A.TERM_END_DT_SID_CATGRYNO,
A.TERM_BEG_DT_CATGRYNO,
A.STUDENT_SUCCESSNO,
A.LOW_TERM_GPA_IND,
A.PARTTIME_TERM_IND,
A.NOT_REG_TERM_IND,
A.WITHDRWL_TERM_IND,
A.FULLTIME_TERM_IND,
A.OVR_12HR_TERM_IND
FROM UF_R3_ANALYSIS_LASTMAJOR2007 A INNER JOIN UFDL_PWCBA_DATA.uf_r3_analysis_grasusdate2007 B
ON A.PERSON_SID = B.PERSON_SID
AND A.TERM_BEG_DT_SID = B.TERM_BEG_DT_SID
AND A.TERM_SID = B.TERM_SID
AND A.ACAD_PROG_PRIMARY = B.ACAD_PROG_PRIMARY
WHERE A.TERM_BEG_DT_SID<20150000
)
